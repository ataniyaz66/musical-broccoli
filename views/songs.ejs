<%- include('layout', { body: `
<h2>All Songs</h2>
<div class="grid">
${songs.map(s => `
<div class="card">
<h3>${s.title}</h3>
<p>${s.artist}</p>
<a class="btn" href="/edit/${s._id}">Edit</a>
<form method="POST" action="/delete/${s._id}">
<button class="danger">Delete</button>
</form>
</div>
`).join('')}
</div>
` }) %>